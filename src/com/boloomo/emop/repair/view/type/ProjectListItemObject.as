package com.boloomo.emop.repair.view.type
{
	/*
	厂修中所包含的工程单
	1. 船舶名称
	2. 工程单编号
	3. 状态 	a) 未开始 	b) 修理中：对应于原来的部分完工。	c) 已完工
	4. 完工日期：YYYY-MM-DD 
	*/
	public class ProjectListItemObject
	{
		protected var _ShipName:String;
		protected var _ProductID:Number;
		protected var _State:String;
		protected var _FinishTime:String;
		
		public function ProjectListItemObject(){
			
		}

		public function get ShipName():String
		{
			return _ShipName;
		}

		public function set ShipName(value:String):void
		{
			_ShipName = value;
		}

		public function get ProductID():Number
		{
			return _ProductID;
		}

		public function set ProductID(value:Number):void
		{
			_ProductID = value;
		}

		public function get State():String
		{
			return _State;
		}

		public function set State(value:String):void
		{
			_State = value;
		}

		public function get FinishTime():String
		{
			return _FinishTime;
		}

		public function set FinishTime(value:String):void
		{
			_FinishTime = value;
		}


	}
}